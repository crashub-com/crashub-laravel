{
  "name": "crashub-com/crashub-laravel",
  "description": "Crashub client for your Laravel project",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "logging",
    "reporting",
    "debugging",
    "monitoring",
    "errors",
    "exceptions"
  ],
  "require": {
    "guzzlehttp/guzzle": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "Crashub\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Crashub\\CrashubServiceProvider"
      ],
      "aliases": {
        "Crashub": "Crashub\\Facades\\Crashub"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^6.19"
  },
  "autoload-dev": {
    "psr-4": {
      "Crashub\\Tests\\": "tests"
    }
  }
}
